<?xml version="1.0" encoding="UTF-8" ?>
<project name="maven-antrun-" default="main"  >
<target name="main">
  <echo>create (or clear) output directory for generated files</echo>
  <mkdir dir="/Users/santosh/oyo/backend-stubs/target/generated-sources"/>
  <delete>
    <fileset includes="**/*" dir="/Users/santosh/oyo/backend-stubs/target/generated-sources"/>
  </delete>
  <echo>generate java source files to /Users/santosh/oyo/backend-stubs/target/generated-sources
									from
									/Users/santosh/oyo/backend-stubs/src/main/thrift/main.thrift</echo>
  <exec executable="thrift">
    <arg value="-r"/>
    <arg value="--gen"/>
    <arg value="java:beans"/>
    <arg value="-o"/>
    <arg value="/Users/santosh/oyo/backend-stubs/target/generated-sources"/>
    <arg value="/Users/santosh/oyo/backend-stubs/src/main/thrift/main.thrift"/>
  </exec>
</target>
</project>